{
  "name": "Cursor MCP Plugin",
  "id": "cursor-mcp-plugin",
  "api": "1.0.0",
  "main": "dist/code.js",
  "ui": "dist/ui.html",
  "documentAccess": "dynamic-page",
  "capabilities": ["inspect"],
  "enableProposedApi": true,
  "enablePrivatePluginApi": true,
  "editorType": ["figma", "figjam", "dev"],
  "networkAccess": {
    "allowedDomains": ["https://google.com"],
    "devAllowedDomains": [
      "http://localhost:3055",
      "ws://localhost:3055"
    ]
  },
  "permissions": [
    "currentuser"
  ]
}
